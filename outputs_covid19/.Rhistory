target <- c("TOTAL_POSITIVOS", "TOTAL_PRUEBAS")
acumulado1<-filter(acumulado1, variable %in% target)
setwd("~/covid19/outputs_covid19/")
png(filename="20200407_pruebas_vs_positivos_COVID19.png")
